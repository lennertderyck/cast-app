{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron . & npm run sass",
    "sass": "sass --watch sass/main.scss:static/css/main.min.css --style compressed",
    "packageDarwin": "electron-packager . 'electron-boilerplate' --icon=static/images/favicon.icns --overwrite",
    "packageWin32": "electron-packager . 'electron-boilerplate' --icon=static/images/favicon.ico --overwrite --platform=win32 --arch=x64 --win32metadata.CompanyName=lennertDeRyck --win32metadata.ProductName=ElectronBoilerplate  --win32metadata.FileDescription=ElectronBoilerplate --win32metadata.OriginalFilename=ElectronBoilerplate.exe --win32metadata.InternalName=ElectronBoilerplate"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^18.3.7"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1",
    "browser-sync": "^2.26.12",
    "chromecast-api": "^0.3.4",
    "cutleryjs": "^2.1.4",
    "electron-packager": "^15.0.0",
    "find": "^0.3.0",
    "movie-info": "^2.0.5",
    "node-media-server": "^2.2.2",
    "path": "^0.12.7",
    "sass": "^1.26.10",
    "serve-static": "^1.14.1",
    "sesam-collapse": "^5.6.0"
  }
}
